    "import yfinance as yf\n",
    "\n",
    "weg = yf.download(\"WEGE3.SA\")['Adj Close']\n",
    "\n",
    "weg = weg.resample(\"Y\").last().pct_change().dropna()\n",
    "\n",
    "weg.index = weg.index.year\n",
    "\n",
    "weg"
